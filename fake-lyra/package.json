{
  "name": "fake-lyra",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn build && yarn run link-public && node ./bin/www",
    "build": "yarn run clean && gulp build",
    "link-public": "./scripts/create-public-dir.sh --link",
    "deploy": "yarn run clean && ./scripts/create-public-dir.sh --deploy && NODE_ENV=production gulp deploy",
    "lint": "eslint ./src/; exit 0",
    "clean": "rm -rf build && cd public && rm -rf app cc stylesheets workers",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha test",
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml ./node_modules/.bin/mocha --reporter mocha-jenkins-reporter",
    "postinstall": "../tools/deployment/ln-project.sh aquila potato",
    "watch": "nodemon -e js,scss --watch src --exec 'yarn start'"
  },
  "dependencies": {
    "@sentry/node": "^7.119.0",
    "@sentry/tracing": "^7.119.0",
    "aws-sdk": "^2.1687.0",
    "axios": "^1.7.6",
    "babel-preset-stage-1": "^6.24.1",
    "bluebird": "^3.5.1",
    "body-parser": "~1.20.2",
    "config": "^1.20.1",
    "connect-datadog": "0.0.9",
    "cookie-parser": "~1.4.6",
    "debug": "~2.6.9",
    "detect-browser": "^2.0.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^8.3.0",
    "localforage": "^1.9.0",
    "lodash": "^4.17.4",
    "lru-cache": "^5.1.1",
    "mustache-express": "^1.2.7",
    "promise": "^7.1.1",
    "redis": "~2.8.0",
    "sass": "^1.77.8"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.3.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^6.1.2",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "mocha": "^7.2.0",
    "nodemon": "^1.18.3",
    "uglify-es": "^3.3.9"
  },
  "resolutions": {
    "dot-prop": "5.1.1",
    "graceful-fs": "^4.2.2",
    "json-schema": "0.4.0",
    "set-value": "^4.1.0"
  }
}
